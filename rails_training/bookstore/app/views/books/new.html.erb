<%= form_with model: @book, class: "nifty_form" do |f| %>
  <%= label_tag(:title, "book title:")%>
  <%= f.text_field :title %>
  <%= label_tag(:count,"book count")%>
  <%= f.text_field :count %>
  <%= f.submit "Create" %>
<% end %>